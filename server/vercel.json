{
    "builds": [
        {
          "src": "server.js",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/socket.io/(.*)", 
          "dest": "index.js"
        },
        {
          "src": "/(.*)",
          "dest": "/server.js"
        }
      ]
      
}
