{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/notes",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/users",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/notes/filter",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/notes/:id",
      "dest": "/api/index.js"
    }
  ]
}